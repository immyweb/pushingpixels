			<div class="contact__content__bkgnd">
				<div class="contact__content__frgnd">
					<div class="contact__content__copy">
						<h1>{{ contact.heading }}</h1>

						<p>{{ contact.copy1 }}</p>

						<div class="contact__content__links">
							<p class="linkedin">
								<a href="{{ contact.linkedinUrl }}">
									<img src="{{ contact.linkedinIcon }}" alt="{{ contact.linkedIconAlt }}">
									{{ contact.linkedin }}
								</a>
							</p>

							<p class="cv">
								<a href="{{ contact.cvUrl }}">
									<img src="{{ contact.cvIcon }}" alt="{{ contact.cvIconAlt }}">
									{{ contact.cv }}
								</a>
							</p>
						</div>

						<p class="contact__content__copy2">{{ contact.copy2 }}</p>

						<form class="contact__content__form" id="contactForm">
							<fieldset>
								<div class="contact__content__form__item">
									<label for="name">{{ contact.form.name }}</label>
									<input type="text" name="name" id="name" placeholder="{{ contact.form.name }}" required>
								</div>
								<div class="contact__content__form__item">
									<label for="email">{{ contact.form.email }}</label>
									<input type="email" name="email" id="email" placeholder="{{ contact.form.email }}" required>
								</div>
								<div class="contact__content__form__item">
									<label for="message">{{ contact.form.message }}</label>
									<textarea name="message" id="message" placeholder="{{ contact.form.message }}" required></textarea>
								</div>
								<div class="contact__content__form__submit">
									<input type="submit" name="submit" value="{{ contact.form.send }}">

									<span class="contact__content__form__error">{{ contact.form.error | safe }}</span>
									<span class="contact__content__form__success">{{ contact.form.success }}</span>
								</div>
							</fieldset>
						</form>
					</div>
				</div>
			</div>
		</div>

		<div class="contact__ellipsecorner--left">
			<div class="contact__ellipsecorner--left__inner"></div>
		</div>

		<div class="contact__ellipsecorner--right">
			<div class="contact__ellipsecorner--right__inner"></div>
		</div>

	</div>

</section>
